{
    "version": 2,
    "builds": [
      {
        "src": "GenAnnot/wsgi.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.11" }
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "GenAnnot/wsgi.py" }
    ]
  }